    "vitepress": "^1.1.4",
    "vue-loader": "^17.4.2",
    "vue-tsc": "^2.0.17",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
