{
  "name": "fake-ui",
  "version": "1.0.0",
  "description": "渐进式的vue3前端组件库构建学习",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/fakeui.min.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "build": "webpack -c ./config/webpack.prod.config.js",
    "build:dev": "webpack -c ./config/webpack.dev.config.js",
    "dev": "webpack serve -c ./config/webpack.dev.config.js",
    "prettier": "prettier -c --write **/*",
    "build:types": "vue-tsc --noEmit && vue-tsc --emitDeclarationOnly --allowJs false"
  },
  "keywords": [
    "组件库",
    "Vue3",
    "渐进式学习"
  ],
  "author": "sqing",
  "license": "ISC",
